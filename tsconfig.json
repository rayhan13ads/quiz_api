{
  "compilerOptions": {
    "target": "ES2016",
    "module": "commonjs",
    "lib": [
      "ES6",
      "ES2016"
    ],
    "allowJs": true,
    "outDir": "build",
    "rootDir": "src",
    "strict": false,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true

  }
}